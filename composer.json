{
    "name": "robbielove/revisionable",
    "description": "Keep a revision history for your models without thinking, created as a package for use with Laravel",
    "keywords": ["model", "laravel", "revision", "history"],
    "authors": [
        {
            "name": "Robbie Love",
            "homepage": "http://robbiecloud.com",
            "role": "Developer"
        }
    ],
    "homepage": "http://github.com/robbielove/revisionable",
    "license": "Private",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4.0|~5.0|~5.1|^6.0|^7.0|^8.0|^9.0",
        "laravel/framework": "~5.4|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Robbielove\\Revisionable": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robbielove\\Revisionable\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Robbielove\\Revisionable\\RevisionableServiceProvider"
            ]
        }
    }
}
